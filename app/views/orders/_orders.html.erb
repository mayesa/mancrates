<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Order ID</th>
      <th>Customer Name</th>
      <th>Order Status</th>
      <th>Total</th>
    </tr>
  </thead>
  <tbody>
    <% orders.each do |order| %>
      <tr>
        <td><%= l order.created_at %></td>
        <td><%= order.id %></td>
        <td><%= order.customer_name %></td>
        <td><%= order.status %></td>
        <td><%= number_to_currency order.total %></td>
      </tr>
    <% end %>
  </tbody>
</table>
